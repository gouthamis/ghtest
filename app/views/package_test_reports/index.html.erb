<p id="notice"><%= notice %></p>

<h1>Listing Package Test Reports</h1>

<table>
  <thead>
    <tr>
    	<th>Customer Name</th>
      <th>Customer details</th>
      <th>Testing performed by</th>
      <th>Test date</th>
      <th>Package details</th>
      <th>Manufacture details</th>
      <th>Authorized signature</th>
      <th>Release date</th>
      <th>Packaging group1</th>
      <th>Packaging group2</th>
      <th>Personal present during test</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_test_reports.each do |package_test_report| %>
      <tr>
      	<td><%= package_test_report.client_name %></td>
        <td><%= package_test_report.customer_details %></td>
        <td><%= package_test_report.testing_performed_by %></td>
        <td><%= package_test_report.test_date %></td>
        <td><%= package_test_report.package_details %></td>
        <td><%= package_test_report.manufacture_details %></td>
        <td><%= package_test_report.authorized_signature %></td>
        <td><%= package_test_report.release_date %></td>
        <td><%= package_test_report.packaging_group1 %></td>
        <td><%= package_test_report.packaging_group2 %></td>
        <td><%= package_test_report.personal_present_during_test %></td>
        <td><%= link_to 'Show', package_test_report %></td>
        <td><%= link_to 'Edit', edit_package_test_report_path(package_test_report) %></td>
        <td><%= link_to 'Destroy', package_test_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package test report', new_package_test_report_path %>
